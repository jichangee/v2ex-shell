  "description": "一个基于 Node.js 的 V2EX 命令行工具",
  "main": "index.js",
  "type": "module",
  "bin": {
    "v2ex-shell": "./index.js"
  },
  "scripts": {
    "start": "node index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
